FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0; // Set by setFields
    }

    outlet
    {
        type            zeroGradient; // or inletOutlet
    }

    bottom
    {
        type            symmetryPlane;
    }

    top
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    sides
    {
        type            symmetryPlane;
    }

    hull
    {
        type            zeroGradient;
    }
}

